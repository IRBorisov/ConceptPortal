    "axios": "^1.6.2",
    "js-file-download": "^0.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-intl": "^6.5.5",
    "react-loader-spinner": "^5.4.5",
    "react-router-dom": "^6.20.0",
    "react-select": "^5.8.0",
    "react-tabs": "^6.0.2",
    "react-toastify": "^9.1.3",
    "react-tooltip": "^5.24.0",
    "reagraph": "^4.14.1"
  },
  "devDependencies": {
    "@lezer/generator": "^1.5.1",
