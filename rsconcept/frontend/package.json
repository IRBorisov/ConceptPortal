    "@uiw/codemirror-themes": "^4.23.6",
    "@uiw/react-codemirror": "^4.23.6",
    "axios": "^1.7.8",
    "clsx": "^2.1.1",
    "html-to-image": "^1.11.11",
    "js-file-download": "^0.4.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-icons": "^5.4.0",
    "react-intl": "^7.0.1",
    "react-loader-spinner": "^6.1.6",
    "react-router": "^7.0.2",
    "react-select": "^5.8.3",
    "react-tabs": "^6.0.2",
    "react-toastify": "^10.0.6",
    "react-tooltip": "^5.28.0",
