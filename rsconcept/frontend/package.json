    "axios": "^1.5.1",
    "js-file-download": "^0.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-intl": "^6.4.7",
    "react-loader-spinner": "^5.4.5",
    "react-router-dom": "^6.16.0",
    "react-select": "^5.7.5",
    "react-tabs": "^6.0.2",
    "react-toastify": "^9.1.3",
    "react-tooltip": "^5.21.5",
    "reagraph": "^4.14.1"
